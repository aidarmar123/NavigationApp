<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CacheAppMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CacheAppMaui"
    xmlns:pages="clr-namespace:CacheAppMaui.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="CacheAppMaui">

    <FlyoutItem>
        <ShellContent Title="Primary"
                      ContentTemplate="{DataTemplate local:MainPage}"/>
        <ShellContent Title="Medicament"
               ContentTemplate="{DataTemplate pages:ListMedicament}"/>
    </FlyoutItem>
</Shell>
